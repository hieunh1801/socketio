<html>

<head>
    <title>Messagee</title>
    <script src="jquery-3.4.1.min.js"></script>
    <script src="socket.io/socket.io.js"></script>
    <script src="app2.js"></script>
    <link rel="stylesheet" href="app2.css">
</head>

<body>
    <div id="wrapper">
        <h2>Welcome to chat room</h2>
        <div id="loginForm">
            <h3>What is your name?</h3>
            <input id="txtUsername" type="text">
            <input type="button" value="register" id="btnRegister">
        </div>

        <div id="chatForm">
            <div id="left">
                <div id="boxTitle">User online</div>
                <div id="boxContent">
                </div>
            </div>
            <div id="right">
                <div id="userInfo">
                    Hello <span id="currentUserName">Username</span>
                    <input type="button" value="logout" id="btnLogout">
                </div>
                <div id="listMessage">

                </div>
                <div id="userTyping"></div>
                <div id="inputMessage">
                    <input type="text" id="txtMessage">
                    <input type="button" id="btnSendMessage" value="Send Message">
                </div>
            </div>
        </div>
    </div>

</body>


</html>